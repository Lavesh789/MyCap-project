#include<iostream>
using namespace std;
int main()
{
    int age;
    cout<<"Enter Age of a user:";
    cin>>age;
    
    if(age>=18)
    {
        cout<<"\nYou are eligible for Voting";
    }
    else
       cout<<"\nYou are not eligible for Voting";
       cout<<"\nWait for "<<18-age<<" year.";
       
    return 0;
}
